{
  "name": "iyb-components",
  "version": "1.0.3",
  "description": "common components",
  "keywords": [
    "components"
  ],
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:wuzhongwei/react-marquee.git"
  },
  "author": "wuzhongwei@iyunbao.com",
  "license": "MIT",
  "scripts": {
    "dev": "node ./scripts/server.js",
    "build:lib": "babel components --out-dir lib --presets=es2015,stage-0,react --plugins=transform-object-assign --ignore 'components/__tests__'",
    "build:css": "cd scripts/gulp && gulp",
    "build:umd": "webpack --config ./scripts/webpack/webpack.config.umd.js && ENV=production webpack --config ./scripts/webpack/webpack.config.umd.js",
    "build": "npm run clean && npm run build:lib && npm run build:css",
    "lint:js": "eslint --ext .jsx --ext .js components; exit 0",
    "lint:style": "stylelint **/*.scss; exit 0",
    "lint": "npm run lint:js && npm run lint:style",
    "clean": "rm -rf lib dist",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "prop-types": "^15.6.1",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-router": "^3.0.2",
    "zarm": "^1.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^8.4.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "file-loader": "^1.1.11",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-filesize": "0.0.6",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^2.6.4",
    "html-webpack-plugin": "^2.30.1",
    "lint-staged": "^6.0.0",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.4",
    "pre-commit": "^1.2.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.9.2"
  },
  "lint-staged": {
    "{components,examples}/**/*.{js,jsx}": "eslint",
    "{components,examples}/**/*.scss": "stylelint"
  },
  "pre-commit": [
    "lint-staged"
  ]
}
